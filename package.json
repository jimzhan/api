{
  "type": "module",
  "name": "backoffice",
  "version": "24.2.0",
  "description": "An opinionated API application boilerplate for Backoffice",
  "main": "index.js",
  "scripts": {
    "genkey": "./node_modules/@fastify/secure-session/genkey.js > ./config/secret.key",
    "migrate": "knex migrate:latest --debug",
    "migrate:up": "knex migrate:up",
    "migrate:down": "knex migrate:down",
    "migrate:rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "lint": "prettier --write src && eslint src",
    "lint:fix": "yarn lint --fix",
    "start": "node src/index.js",
    "test": "cross-env NODE_ENV=test vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jimzhan/api.git"
  },
  "keywords": [
    "ESM",
    "fastify",
    "boilerplate",
    "API"
  ],
  "author": "Jim Zhan <jim.zhan@me.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/jimzhan/api/issues"
  },
  "homepage": "https://github.com/jimzhan/api#readme",
  "engines": {
    "node": "^18.18 || ^20.8"
  },
  "devDependencies": {
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4"
  },
  "dependencies": {
    "@fastify/passport": "^2.4.0",
    "@fastify/secure-session": "^7.1.0",
    "@fastify/under-pressure": "^8.3.0",
    "bcrypt": "^5.1.1",
    "config": "^3.3.11",
    "cross-env": "^7.0.3",
    "fastify": "^4.26.1",
    "knex": "^3.1.0",
    "objection": "^3.1.4",
    "pg": "^8.11.3",
    "ulidx": "^2.3.0",
    "vitest": "^1.3.1",
    "winston": "^3.11.0"
  }
}
